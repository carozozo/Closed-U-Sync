<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
  <name>*dbname*</name>
  <create>true</create>
  <overwrite>false</overwrite>
  <charset>latin1</charset>
  <table>
    <name>*dbprefix*contact</name>
    <declaration>
      <field>
        <name>uid</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>50</length>
      </field>
      <field>
        <name>id</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>50</length>
      </field>
      <field>
        <name>nickname</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>50</length>
      </field>
      <index>
        <name>uid_id_index</name>
        <primary>true</primary>
        <field>
          <name>uid</name>
          <sorting>ascending</sorting>
        </field>
        <field>
          <name>id</name>
          <sorting>ascending</sorting>
        </field>
      </index>
    </declaration>
  </table>
  <table>
    <name>*dbprefix*contact_group</name>
    <declaration>
      <field>
        <name>id</name>
        <type>integer</type>
        <default>0</default>
        <notnull>true</notnull>
        <autoincrement>1</autoincrement>
        <unsigned>true</unsigned>
        <length>4</length>
      </field>
      <field>
        <name>uid</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>50</length>
      </field>
      <field>
        <name>name</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>50</length>
      </field>
      <field>
        <name>contactId</name>
        <type>text</type>
        <notnull>false</notnull>
        <length>255</length>
      </field>
      <index>
        <name>id_index</name>
        <primary>true</primary>
        <field>
          <name>id</name>
          <sorting>ascending</sorting>
        </field>
      </index>
      <index>
        <name>uid_name_index</name>
        <unique>true</unique>
        <field>
          <name>uid</name>
          <sorting>ascending</sorting>
        </field>
        <field>
          <name>name</name>
          <sorting>ascending</sorting>
        </field>
      </index>
    </declaration>
  </table>
</database>