<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>latin1</charset>
	<table>
		<name>*dbprefix*notification</name>
		<declaration>
			<field>
				<name>sn</name>
				<type>integer</type>
				<autoincrement>1</autoincrement>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<field>
				<name>publisher</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>uid</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>message</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>255</length>
			</field>
			<field>
				<name>readed</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>1</length>
			</field>
			<field>
				<name>createDate</name>
				<type>timestamp</type>
				<notnull>true</notnull>
			</field>
			<index>
		    	<name>notification_pKey</name>
    			<primary>true</primary>
    			<field>
     				<name>sn</name>
     				<sorting>ascending</sorting>
    			</field>
			</index>
		</declaration>
	</table>
</database>
